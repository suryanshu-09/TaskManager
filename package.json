    "expo": "~54.0.30",
    "expo-auth-session": "^7.0.10",
    "expo-constants": "~18.0.12",
    "expo-crypto": "^15.0.8",
    "expo-dev-client": "~6.0.13",
    "expo-device": "~8.0.9",
